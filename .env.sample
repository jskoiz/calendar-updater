# CalDAV Server URL
CALDAV_URL=https://example.com/caldav/

# CalDAV Credentials
CALDAV_USERNAME=your_username
CALDAV_PASSWORD=your_password

# Timezone for events 
TIMEZONE=UTC

# Name of the calendar to add events to
CALENDAR_NAME=YourCalendarName

# Log level (debug, info, warn, error)
LOG_LEVEL=info